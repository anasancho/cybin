require 'cybin'
require 'table-extended'
P=Pat
_=P{}
if m==nil then m=Metro() end
if s==nil then s=Synth() end
function __process(sr)
   m:Process(sr)
   return s:Process(sr)
end
function play(t)
   m:Pat(P{P{0,1,2}:slow(1),3,7},function(x) s:PlayNote(60+x) end,1)
   return 1
end

m:Interval("play")

