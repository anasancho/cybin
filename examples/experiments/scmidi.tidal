n "0 0 0 0" - 12,
n "~ 12 ~ 12",
fast 4 $ n "~ 48" * gain 0.7,
n "24(5,16,2)"
]) |+ "midi" |+ n "<0 3 5 [-2 -1] 0 -4 3 [2 1]>"

