cybin.loadfragmentshaderfile("raymarch.frag")
sphere_width=cybin.getuniformid("sphere_width")
sample=0
function __process(sr)
   local lfo=math.abs(math.sin(sample*math.pi/sr))
   sample=sample+1
   cybin.setuniform1f(sphere_width,lfo*1.4)
   local output=math.random()*lfo
   return output,output
end


